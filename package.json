{
  "name": "commit-release",
  "description": "Commit and tag a release for a conventional changelog project.",
  "version": "0.8.0",
  "author": "Jamie Mason (https://github.com/JamieMason)",
  "bin": {
    "commit-release": "./index.js"
  },
  "bugs": {
    "url": "https://github.com/JamieMason/commit-release/issues"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "conventional-recommended-bump": "0.3.0",
    "execa": "0.5.0",
    "generate-changelog": "1.2.1",
    "graceful-fs": "4.1.11",
    "package-json-to-readme": "1.5.1",
    "when": "3.7.7"
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "xo": "0.17.1"
  },
  "homepage": "https://github.com/JamieMason/commit-release",
  "issues": "https://github.com/JamieMason/commit-release/issues",
  "keywords": [
    "changelog",
    "commit",
    "deploy",
    "git",
    "log",
    "release",
    "workflow"
  ],
  "license": "MIT",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/JamieMason/commit-release.git"
  },
  "scripts": {
    "lint": "xo --fix"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "esnext": false,
    "space": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
